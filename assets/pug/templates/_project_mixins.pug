mixin header

	header.header
		.fog
		.header_in
			a.logo_link(href="./")
				img.logo_img(src="i/header-logo.png" alt="logo")
				.logo_text hiking<span class="accent_mod">trip</span>
			nav.header_nav
				ul.header_nav_list
					li.header_nav_item
						a.header_nav_link.active_page(href="#") Acasă
					li.header_nav_item
						a.header_nav_link(href="#") Servicii
					li.header_nav_item
						a.header_nav_link(href="#") Despre noi
					li.header_nav_item
						a.header_nav_link(href="#") Cele mai apropiate excursii
					li.header_nav_item
						a.header_nav_link(href="#") Galerie
					li.header_nav_item
						a.header_nav_link(href="#") Feedback
					li.header_nav_item
						a.header_nav_link(href="#") Contacte
				.select_language
					+form_element({
						type: 'select',
						class_array: language_field,
						title: '',
						id: 'language',
						options: ['Ro', 'En', 'Ru']
					})
			.menu_trigger.menuTrigger
				span.menu_trigger_decor
mixin sub_header(array)
	.sub_header(class= array.mod)
		.subheader_bg_wrapper
			img.subheader_bg_img(src= array.bgImg alt="")
		if array.titleText
			.subheader_content
				.content_wrapper
					h1.subheader_title= array.titleText
					.subheader_subtile= array.subtitleText
					a.button.default_mod(href="#")= array.btnText
					.mouse
						.scroll
mixin servises_list
